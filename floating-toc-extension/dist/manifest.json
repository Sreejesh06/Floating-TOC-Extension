{
  "manifest_version": 3,
  "name": "Floating TOC",
  "version": "1.0",
  "description": "A floating table of contents",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "css": ["styles.css"]
  }],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}